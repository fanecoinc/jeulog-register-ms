model Cart {
  id              String     @id
  plate           String     @unique
  isActive        Boolean
  createdAt       DateTime
  updatedAt       DateTime?
  usedAsCartOne   TruckSet[] @relation("CartOne")
  usedAsCartTwo   TruckSet[] @relation("CartTwo")
  usedAsCartThree TruckSet[] @relation("CartThree")
}
